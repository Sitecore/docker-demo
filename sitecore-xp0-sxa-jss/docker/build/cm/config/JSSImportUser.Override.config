<!--
  Purpose: This include file set's the default JSS Import user to the sitecore\admin user
           to work around an permissions issue. ONLY USE THIS IN DEVELOPMENT ENVIRONMENTS.
-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <javaScriptServices>
      <apps>
        <app name="defaults"
          set:importUser="sitecore\admin"
        />
      </apps>
    </javaScriptServices>
  </sitecore>
</configuration>